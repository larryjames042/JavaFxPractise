<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefWidth="867.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxpractise.controller.StudentController">
   <top>
      <HBox prefHeight="66.0" prefWidth="600.0" style="-fx-background-color: #08043b;" BorderPane.alignment="CENTER">
         <children>
            <AnchorPane prefHeight="66.0" prefWidth="869.0">
               <children>
                  <TextField fx:id="searchTextField" layoutX="60.0" layoutY="20.0" prefHeight="26.0" prefWidth="155.0" />
                  <Button fx:id="buttonSearch" layoutX="254.0" layoutY="20.0" mnemonicParsing="false" text="Add New Student" />
                  <Label layoutX="14.0" layoutY="25.0" text="Search" textFill="WHITE" />
               </children>
            </AnchorPane>
         </children></HBox>
   </top>
   <center>
      <TableView fx:id="studentTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
